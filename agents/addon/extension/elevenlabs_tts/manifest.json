{
    "type": "extension",
    "name": "elevenlabs_tts",
    "version": "0.1.0",
    "language": "go",
    "dependencies": [
        {
            "type": "system",
            "name": "rte_runtime",
            "version": "0.1.0"
        }
    ],
    "api": {
        "property": {
            "api_key": {
                "type": "string"
            },
            "model_id": {
                "type": "string"
            },
            "request_timeout_seconds": {
                "type": "int64"
            },
            "voice_id": {
                "type": "string"
            }
        },
        "data_in": [
            {
                "name": "text_data",
                "property": {
                    "text": {
                        "type": "string"
                    }
                }
            }
        ],
        "cmd_in": [
            {
                "name": "flush"
            }
        ],
        "cmd_out": [
            {
                "name": "flush"
            }
        ],
        "pcm_frame_out": [
            {
                "name": "pcm_frame"
            }
        ]
    }
}
